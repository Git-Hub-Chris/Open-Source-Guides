---
layout: null
---
<?xml version="1.0" encoding="utf-8"?>
<rss version="2.0"
  xmlns:content="http://purl.org/rss/1.0/modules/content/"
  xmlns:wfw="http://wellformedweb.org/CommentAPI/"
  xmlns:dc="http://purl.org/dc/elements/1.1/"
  xmlns:atom="http://www.w3.org/2005/Atom"
  xmlns:sy="http://purl.org/rss/1.0/modules/syndication/"
  xmlns:slash="http://purl.org/rss/1.0/modules/slash/"
  >

  <channel>
    <title xml:lang="en">GitHub Guides</title>
    <atom:link type="application/atom+xml" href="https://guides.github.com/feed/index.xml" rel="self"/>
    <link>https://guides.github.com/</link>
    <pubDate>{{ site.time | date_to_rfc822 }}</pubDate>
    <lastBuildDate>{{ site.time | date_to_rfc822 }}</lastBuildDate>
    <language>en-US</language>
    <generator>GitHub Guides</generator>
    <description>Learn how to do more with GitHub.</description>

   {% for post in site.pages %}
     {% if post.title != nil %}
     <item>
       <title>{{ post.title | xml_escape }}</title>
       {% if post.type == 'link' %}
         <link>{{ post.link }}</link>
       {% else %}
         <link>http://guides.github.com{{ post.url }}</link>
       {% endif %}
       <pubDate>{{ post.updated_at | date_to_rfc822 }}</pubDate>
       <guid isPermaLink="true">http://guides.github.com/{{ post.path }}</guid>
       <description>{{ post.content | xml_escape }}</description>
     </item>
     {% endif %}
   {% endfor %}

  </channel>
</rss>
